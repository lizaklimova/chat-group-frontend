{
  "name": "chat-group-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "https-preview": "serve dist",
    "prepare": "husky",
    "type-check": "bash -c tsc --pretty --noEmit",
    "format": "prettier --write --cache \"**/*.[tj]s?(x)\"",
    "lint:check": "eslint --max-warnings=0 --no-ignore \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "eslint . --fix",
    "dep-cruiser:preview": "npx depcruise src --include-only \"^src\" --output-type dot | dot -T svg > dependency-graph-preview.svg"
  },
  "dependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@mantine/core": "^7.13.4",
    "@mantine/hooks": "^7.13.4",
    "@reduxjs/toolkit": "^2.3.0",
    "@tabler/icons-react": "^3.20.0",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.27.0",
    "redux-persist": "^6.0.0",
    "typescript-eslint": "^8.11.0",
    "vite-plugin-checker": "^0.7.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@neodx/svg": "^0.7.1",
    "@total-typescript/ts-reset": "0.5.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.8.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.9.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "dependency-cruiser": "^16.5.0",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.1",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "playwright": "^1.48.2",
    "postcss": "^8.4.47",
    "postcss-nesting": "^12.1.5",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-pwa": "^0.17.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --max-warnings=0 --no-ignore"
    ]
  }
}